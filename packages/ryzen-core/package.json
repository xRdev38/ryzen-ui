    "@stencil/postcss": "^2.1.0",
    "@stencil/sass": "^1.4.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/puppeteer": "^5.4.3",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "puppeteer": "^20.7.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "sass": "^1.64.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.9",
    "postcss-cli": "^9.0.1",
    "postcss-rtlcss": "^3.6.3",
    "puppeteer": "^13.0.1",
    "sass-loader": "^10.2.1",
    "semantic-release": "^17.4.2",
    "ts-loader": "9.2.1",
    "web-component-analyzer": "^1.1.6"
